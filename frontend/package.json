{
  "name": "inspect-hub-frontend",
  "version": "1.0.0",
  "description": "Inspect-Hub AML Compliance Monitoring System - Frontend",
  "private": true,
  "type": "module",
  "author": "Inspect-Hub Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "nuxt dev --host 0.0.0.0 --port 3000",
    "dev:open": "nuxt dev --host 0.0.0.0 --port 3000 --open",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "clean": "rm -rf .nuxt .output dist node_modules/.cache",
    "clean:modules": "rm -rf node_modules package-lock.json && npm install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "audit": "npm audit --production",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated",
    "update:check": "npx npm-check-updates",
    "update:interactive": "npx npm-check-updates -i",
    "prepare": "nuxt prepare",
    "generate:component": "node scripts/generate-component.mjs",
    "generate:validate": "node scripts/validate-build.mjs"
  },
  "dependencies": {
    "@fontsource/noto-sans-kr": "^5.2.8",
    "@nuxtjs/i18n": "^10.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.9.0",
    "@primevue/themes": "^4.1.0",
    "@vee-validate/zod": "^4.13.2",
    "echarts": "^5.5.1",
    "nuxt": "4.2.0",
    "pinia": "^2.2.6",
    "primevue": "^4.1.0",
    "vee-validate": "^4.13.2",
    "vue-echarts": "^7.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.5.2",
    "@nuxt/eslint": "^0.5.7",
    "@playwright/test": "^1.56.1",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "happy-dom": "^20.0.10",
    "msw": "^2.12.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vitest": "^4.0.7",
    "vue-tsc": "^2.1.10"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}